                                    <img src="images/Durol.jpg">
                                </div>
                               </a> 
                              
                                <figcaption class="info-wrap border-top">
                                    <div class="price-wrap">
                                        <strong class="price">GH₵ <%= (row.Selling_price) %></strong>
                                    </div>
                                    <!-- price-wrap.// -->
                                    <p class="title mb-2"><%= row.Product_Name %></p>

                                    <div class="">
                                        <div class="Addtocartbtn">
                                            <a  class="btn btn-sm btn-outline-success ">  Add to cart</a>
                                            <span style="display: none;"><%= row.ProductID %></span>
                                            <span style="display: none;"><%= row.Selling_price %></span>
                                            <span style="display: none;"><%= row.Product_Name %></span>
                                            <span style="display: none;"><%= row.fileID %></span>
                                            <span style="display: none;"><%= row.userProductstId %></span>

                                            <span style="display: none;"><%= row.NameID %></span>
                                            <span style="display: none;"><%= row.BrandID %></span>
                                        
